{
    "name": "SAVR",
    "description": "Helps you with screen stress",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": ["alarms", "notifications", "storage", "system.memory"],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_title": "Shader",
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "get_started16.png",
      "32": "get_started32.png",
      "48": "get_started48.png",
      "128": "128.png"
    }  
}
